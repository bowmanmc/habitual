{
  "name": "Habitual",
  "description": "Habit Tracking Application",
  "version": "0.2.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "https://*.google.com/*",
    "https://*.google-analytics.com/*",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["analytics.js"]
    }
  ],
  "icons": { 
      "128": "icons/habitual-128.png" 
  },
  "offline_enabled": true
}

