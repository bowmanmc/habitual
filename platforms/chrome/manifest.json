{
  "name": "Habitual",
  "description": "Better Habits, Better You.",
  "version": "0.2.6",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "fullscreen",
    "storage"
  ],
  "icons": {
      "16": "icons/habitual-16.png",
      "48": "icons/habitual-48.png",
      "128": "icons/habitual-128.png",
      "256": "icons/habitual-256.png",
      "512": "icons/habitual-512.png"
  },
  "offline_enabled": true
}

